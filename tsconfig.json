{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "baseUrl": ".",
    "types": ["navermaps"],
    "paths": {
      "@/types/common": ["./node_modules/ondaji_types/common"],
      "@/types/domain": ["./node_modules/ondaji_types/domain"],
      "@/utils/*": ["./*"],
      "@/constants/*": ["./constants/*"]
    },
    "jsxImportSource": "react"
  },
  "include": ["**/*.ts", "**/*.tsx", "../constants/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
